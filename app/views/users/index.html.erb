<ul class="users">
  <% @users.each do |user| %>
    <li>
        <%  if user.firstname and user.lastname %>
            <%= link_to user.firstname + ' ' + user.lastname, user %> signed up <%= user.created_at.to_date %>
        <% else %>
            <%= link_to user.email, user %> signed up <%= user.created_at.to_date %>
        <% end %>
    </li>
  <% end %>
</ul>